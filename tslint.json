{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "no-string-throw": true,
    "no-unused-expression": true,
    "no-duplicate-variable": true,
    "no-var-requires": false,
    "no-console": [
      true,
      "log",
      "warn"
    ],
    "curly": true,
    "class-name": true,
    "semicolon": [
      true,
      "always"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "triple-equals": true,
    "trailing-comma": false
  },
  "defaultSeverity": "error"
}
